{
  "name": "react-redux-unit-testing-code-coverage",
  "version": "0.1.0",
  "description": "React & Redux: Write better unit tests and get 100% code coverage",
  "repository": "https://github.com/camsjams/react-redux-unit-testing-code-coverage.git",
  "scripts": {
    "start": "react-static start",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --ext .jsx --ext .js",
    "lint:css": "stylelint . './src/**/**/*.jsx'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "coverage": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "history": "4.7.2",
    "prop-types": "15.6.2",
    "react": "16.5.1",
    "react-dom": "16.5.1",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-static": "5.9.8",
    "redux": "4.0.0",
    "semantic-ui-css": "2.3.3",
    "semantic-ui-react": "0.82.5",
    "styled-components": "3.4.6"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "1.5.1",
    "eslint-config-react-tools": "1.1.6",
    "eslint": "4.19.1",
    "eslint-loader": "1.8.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.7.0",
    "stylelint": "7.11.1",
    "stylelint-config-standard": "16.0.0",
    "stylelint-processor-styled-components": "0.1.2"
  }
}
